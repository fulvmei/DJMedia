<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.fulvmei.android.media.ui.PlayerView
        android:id="@+id/playerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000"
        app:resize_mode="fit"
        app:shutter_background_color="#000000"
        app:surface_type="texture_view" />

    <com.fulvmei.android.djmedia.demo.main.widget.AppVideoWatermarkView
        android:id="@+id/videoWatermarkView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.fulvmei.android.djmedia.ui.DJVideoImageView
        android:id="@+id/videoImageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.fulvmei.android.djmedia.demo.main.widget.AppVideoControlView
        android:id="@+id/videoControlView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:show_timeout="3000" />

    <com.fulvmei.android.djmedia.ui.DJVideoIdleView
        android:id="@+id/videoIdleView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center" />

    <com.fulvmei.android.djmedia.ui.DJVideoBufferingView
        android:id="@+id/videoBufferingView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center" />

    <com.fulvmei.android.djmedia.ui.DJVideoPlayErrorView
        android:id="@+id/videoPlayErrorView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:focusable="true" />

    <com.fulvmei.android.djmedia.ui.DJVideoEndedView
        android:id="@+id/videoEndedView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true"
        android:focusable="true" />

    <com.fulvmei.android.djmedia.ui.DJVideoPlayWithoutWifiView
        android:id="@+id/noWifiView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.fulvmei.android.djmedia.demo.main.VIPStateView
        android:id="@+id/vipStateView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.fulvmei.android.djmedia.demo.main.LiveStateView
        android:id="@+id/liveStateView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</merge>
